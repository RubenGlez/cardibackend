{
  "exclude": [
    "__tests__",
    ".github",
    ".vscode",
    "coverage",
    "custom_types",
    "data",
    "dist",
    "node_modules",
    "jest.config.ts"
  ],
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "resolveJsonModule": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "typeRoots": ["custom_types", "node_modules/@types"],
  }
}
